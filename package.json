{
  "name": "findr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:hosting": "node scripts/prepare-firebase-host.js",
    "deploy:firebase": "next build && firebase deploy",
    "deploy:firebase:flutter": "cd findr_flutter && flutter build web && firebase deploy",
    "watch:deploy": "chokidar \"findr_flutter/lib/**\" \"findr_flutter/web/**\" \"findr_flutter/pubspec.yaml\" -c \"npm run deploy:firebase:flutter\""
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.8",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.300.0",
    "simplex-noise": "^4.0.3",
    "tailwind-merge": "^2.2.0",
    "firebase": "^12.8.0",
    "leaflet": "^1.9.4",
    "next": "^14.0.0",
    "openai": "^6.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
